{
    "username": "netperf",
    "db_write_queue": "/netperf.db", 
    "bandwidth_monitor": {
        "enabled": true
    }, 
    "speedtest": {
        "data_usage_quota_GB": 0, 
        "enforce_quota": false
    }, 
    "data_root": "/mnt/usb_storage/netperf", 
    "dashboard": {
        "clock_type_24hr": false, 
        "enabled": true, 
        "queue_name": "/netperf.dashboard"
    }, 
    "logging": {
        "log_level": "INFO", 
        "logger_format": "%(asctime)s %(name)s %(levelname)s:%(message)s"
    }
}
